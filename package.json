    "url": "https://github.com/glenjamin/transit-immutable-js/issues"
  },
  "homepage": "https://github.com/glenjamin/transit-immutable-js",
  "peerDependencies": {
    "immutable": ">= 3",
    "transit-js": ">= 0.8"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "chai-immutable": "^1.2.0",
    "coveralls": "^2.11.2",
    "eslint": "^6.6.0",
    "immutable": ">=3.7.4",
    "mocha": "^2.2.5",
